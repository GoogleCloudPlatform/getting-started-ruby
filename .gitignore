# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp

# Ignore database configuration
/config/database.yml

# Ignore generated assets
/public/assets/

# Ignore SQLite database files
*.sqlite3
